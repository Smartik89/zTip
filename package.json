{
  "name": "ztip",
  "version": "2.0.0",
  "description": "A free, ultra lightweight vanilla JS tooltip plugin. It is only ~1KB gzipped. No jQuery!",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --watch --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "laravel-mix": "latest",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awps/zTip.git"
  },
  "keywords": [
    "vanilla",
    "tooltip",
    "tip",
    "popup",
    "qtip",
    "ztip",
    "popover"
  ],
  "author": "Andrew Surdu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awps/zTip/issues"
  },
  "homepage": "https://github.com/awps/zTip#readme"
}
